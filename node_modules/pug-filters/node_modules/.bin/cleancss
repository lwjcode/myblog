#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../._clean-css@3.4.26@clean-css/bin/cleancss" "$@"
  ret=$?
else 
  node  "$basedir/../../../._clean-css@3.4.26@clean-css/bin/cleancss" "$@"
  ret=$?
fi
exit $ret
